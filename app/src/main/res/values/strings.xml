<resources>
    <string name="app_name">翻译</string>
    <string name="subtype_en_US">翻译工具</string>
    <style name="MyTheme" parent="@android:style/Theme.Material">
        <item name="android:actionBarStyle">@style/MyTheme.ActionBarStyle</item>
        <item name="android:actionMenuTextColor">#999</item>
        <item name="android:actionOverflowButtonStyle">@style/MyTheme.Overflow</item>
        <item name="android:navigationBarColor">#000</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">#000</item>
    </style>

    <style name="MyTheme.ActionBarStyle" parent="@android:style/Widget.Material.ActionBar">
        <item name="android:titleTextStyle">@style/MyTheme.ActionBar.TitleTextStyle</item>
    </style>

    <style name="MyTheme.ActionBar.TitleTextStyle" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Title">
        <item name="android:textColor">#999</item>
    </style>
    <style name="MyTheme.Overflow" parent="@android:style/Widget.Material.ActionButton.Overflow">
        <item name="android:tint">#999</item>
    </style>
</resources>